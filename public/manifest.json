{
  "name": "My Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "history",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://tweetdeck.twitter.com/*"
      ],
      "js": [
        "src/tweetdeck/content.js"
      ],
      "css": [
        "src/tweetdeck/style.css"
      ]
    },
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "src/twitter/content.js"
      ]
    },
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "src/youtube/content.js"
      ]
    },
    {
      "matches": [
        "https://www.nicovideo.jp/*",
        "https://live2.nicovideo.jp/*"
      ],
      "js": [
        "src/niconico/content.js"
      ]
    },
    {
      "matches": [
        "https://www.google.com/*",
        "https://www.google.co.jp/*"
      ],
      "js": [
        "src/google/www/content.js"
      ]
    },
    {
      "matches": [
        "https://drive.google.com/*"
      ],
      "js": [
        "src/google/drive/content.js"
      ]
    },
    {
      "matches": [
        "https://colab.research.google.com/*"
      ],
      "js": [
        "src/google/colab/content.js"
      ]
    },
    {
      "matches": [
        "https://accounts.google.com/*"
      ],
      "js": [
        "src/google/oauth/content.js"
      ]
    },
    {
      "matches": [
        "https://translate.google.com/*",
        "https://translate.google.co.jp/*"
      ],
      "js": [
        "src/google/translate/content.js"
      ]
    },
    {
      "matches": [
        "https://www.deepl.com/*"
      ],
      "js": [
        "src/deepl/content.js"
      ]
    },
    {
      "matches": [
        "https://www.dropbox.com/*"
      ],
      "js": [
        "src/dropbox/content.js"
      ],
      "css": [
        "src/dropbox/style.css"
      ]
    },
    {
      "matches": [
        "https://app.slack.com/*"
      ],
      "js": [
        "src/slack/content.js"
      ]
    },
    {
      "matches": [
        "http://jin115.com/*"
      ],
      "js": [
        "src/blog/content.js"
      ]
    }
  ]
}
