{
  "name": "My General Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "tabs",
    "history",
    "downloads",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "src/lib/jquery.min.js",
        "src/youtube/content.js"
      ],
      "css": [
        "src/youtube/style.css"
      ]
    },
    {
      "matches": [
        "https://www.youtube.com/live_chat*"
      ],
      "js": [
        "src/lib/jquery.min.js",
        "src/youtube/live_chat/content.js"
      ],
      "css": [
        "src/youtube/live_chat/style.css"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "src/lib/jquery.min.js",
        "src/twitter/content.js"
      ]
    },
    {
      "matches": [
        "https://www.google.com/*",
        "https://www.google.co.jp/*"
      ],
      "css": [
        "src/google/search/style.css"
      ]
    },
    {
      "matches": [
        "https://drive.google.com/*"
      ],
      "js": [
        "src/lib/jquery.min.js",
        "src/google/drive/content.js"
      ],
      "css": [
        "src/google/drive/style.css"
      ]
    },
    {
      "matches": [
        "https://colab.research.google.com/*"
      ],
      "js": [
        "src/lib/jquery.min.js",
        "src/google/colab/content.js"
      ]
    },
    {
      "matches": [
        "https://accounts.google.com/*"
      ],
      "js": [
        "src/lib/jquery.min.js",
        "src/google/oauth/content.js"
      ]
    },
    {
      "matches": [
        "https://app.slack.com/*"
      ],
      "css": [
        "src/slack/style.css"
      ]
    },
    {
      "matches": [
        "http://jin115.com/*"
      ],
      "css": [
        "src/blog/style.css"
      ]
    }
  ]
}
